            <Label fx:id="mensajito" text="Estas son tus últimas diez reservas,">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="nombreUsuario" text="Popis">
               <HBox.margin>
                  <Insets left="5.0" top="10.0" />
