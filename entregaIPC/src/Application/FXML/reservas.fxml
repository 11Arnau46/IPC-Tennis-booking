<BorderPane prefHeight="750.0" prefWidth="700.0" stylesheets="@../css/reservas.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Application.Controller.reservasController">
